<mixer>
    <!-- These are the initial mixer settings -->
    <ctl name="DMIC Switch" value="0" />
    <ctl name="Headphone Volume" value="0" />
    <ctl name="Headphone Switch" value="0" />
    <ctl name="Mixout Left DAC Left Switch" value="0" />
    <ctl name="Mixout Right DAC Right Switch" value="0" />

    <ctl name="Mixout Left DAC Left Switch" value="0" />
    <ctl name="Mixout Right DAC Right Switch" value="0" />
    <ctl name="Headphone Switch" value="0" />
    <ctl name="Mic 2 Switch" value="0" />
    <ctl name="Mixin Left Mic 2 Switch" value="0" />
    <ctl name="Mixin Right Mic 2 Switch" value="0" />
    <ctl name="Mixin PGA Switch" value="0" />
    <ctl name="Headphone Switch" value="0" />
    <ctl name="Mixout Left DAC Left Switch" value="0" />
    <ctl name="Mixout Right DAC Right Switch" value="0" />

    <!-- These are actual sound device specific mixer settings -->
    <path name="headset-mic">
        <ctl name="DMIC Switch" value="0" />
        <ctl name="Mic 2 Amp Source MUX" value="MIC_P" />
        <ctl name="Mic 2 Switch" value="1" />
        <ctl name="Mixin Left Mic 2 Switch" value="1" />
        <ctl name="Mixin Right Mic 2 Switch" value="1" />
        <ctl name="Mixin PGA Switch" value="1" />
	    <ctl name="ADC Switch" value="1" />
        <ctl name="DAI Left Source MUX" value="ADC Left" />
        <ctl name="DAI Right Source MUX" value="ADC Right" />
        <ctl name="Headphone Volume" value="53, 53" />
        <ctl name="Headphone Switch" value="1" />
        <ctl name="Mixout Left DAC Left Switch" value="1" />
        <ctl name="Mixout Right DAC Right Switch" value="1" />
    </path>

    <path name="headphones">
        <ctl name="Mixout Left DAC Left Switch" value="1" />
        <ctl name="Mixout Right DAC Right Switch" value="1" />
        <ctl name="Headphone Switch" value="1" />
        <ctl name="Headphone Volume" value="53, 53" />
    </path>

</mixer>